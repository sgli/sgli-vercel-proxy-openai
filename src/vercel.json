{ 
    "rewrites": [
        { 
			"source": "/", 
			"destination": "/index.html" 
		}
    ],
	"routes": [
		{
		  "src": "/api/:match*",
		  "dest": "https://api.openai.com/:match*"
		},
		{
		  "src": "/chat/:match*",
		  "dest": "https://chat.openai.com/:match*"
		}
	],
    "github": {
        "silent": true
    }
}
